{
  "id": "6f432af8-1146-11ea-9a9f-362b9e155667",
  "name": "NodeJSRunner",
  "friendlyName": "Node JS Script Runner",
  "description": "Use this task in a build or release pipeline to run a JS script.",
  "author": "Carlos Flor",
  "helpMarkDown": "Use this task in a build or release pipeline to run a JS script.",
  "category": "Utility",
  "visibility": [
    "Build",
    "Release"
  ],
  "demands": [],
  "version": {
    "Major": "0",
    "Minor": "4",
    "Patch": "2"
  },
  "minimumAgentVersion": "1.95.0",
  "instanceNameFormat": "NodeJSRunner $(message)",
  "inputs": [
    {
      "name": "ScriptSource",
      "label": "Script Source",
      "type": "radio",
      "defaultValue": "FilePath",
      "options": {
        "FilePath": "FilePath",
        "Inline": "Inline"
      }
    },
    {
      "name": "ScriptPath",
      "label": "Script path",
      "type": "filePath",
      "required": true,
      "visibleRule": "ScriptSource = FilePath"
    },
    {
      "name": "InlineScript",
      "label": "Inline Script",
      "type": "multiLine",
      "required": true,
      "visibleRule": "ScriptSource = Inline"
    },
    {
      "name": "Arguments",
      "label": "Arguments",
      "type": "string",
      "required": false
    },
    {
      "name": "WorkingDirectory",
      "label": "Working directory",
      "type": "filePath",
      "required": false,
      "groupName": "Advanced"
    }
  ],
  "groups": [
    {
      "name": "Advanced",
      "displayName": "Advanced"
    }
  ],
  "execution": {
    "Node10": {
      "target": "dist/sample.js",
      "argumentFormat": ""
    }
  }
}