    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "INPUT_SCRIPTSOURCE": "Inline",
                "INPUT_INLINESCRIPT": "console.log('Hello World')"
            },
            "program": "${workspaceFolder}\\buildAndReleaseTask\\dist\\sample.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}